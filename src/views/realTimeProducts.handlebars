<h1>Real Time Products</h1>
<table id="productos">
  <tr>
    <th>ID</th>
    <th>Nombre</th>
    <th>Precio</th>
    <th>Stock</th>
    <th>Categor√≠a</th>
  </tr>
  {{#each products}}
